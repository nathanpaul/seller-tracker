{
  "manifest_version": 2,

  "name": "This is the extension I am using to learn how to build Chrome Extension.",
  "description": "This extension will exercise various functionalities.",
  "version": "1.0",
  "key": "",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Super Test"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/",
    "identity",
    "tabs", 
    "https://docs.google.com/feeds/*",
    "https://www.google.com/accounts/OAuthGetRequestToken",
    "https://www.google.com/accounts/OAuthAuthorizeToken",
    "https://www.google.com/accounts/OAuthGetAccessToken"
  ],
  "background": {
    "page": "chrome_ex_oauth.html"
  },
  "oauth2": {
    "client_id": "525774793049-g4535gfratld5lsqo0ip0g3db35jhtnh.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly"
    ]
  }
}
